{
  "name": "bncoder4",
  "displayName": "bncoder4",
  "description": "BNCoder v4",
  "version": "0.17.0",
  "repository": {
    "url": "https://github.com/wolmir/bncoder4"
  },
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configurationDefaults": {
      "[*]": {
        "editor.semanticHighlighting.enabled": true
      }
    },
    "commands": [
      {
        "command": "bncoder4.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "bncoder4.suggestBetterNames",
        "title": "Suggest better names",
        "icon": "$(comment)"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "bncoder4.suggestBetterNames",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "build": "vsce package",
    "package": "npm version minor && npm run build"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.102.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.6.0",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@llamaindex/ollama": "^0.1.20",
    "@llamaindex/readers": "^3.1.18",
    "@llamaindex/workflow": "^1.1.20",
    "llamaindex": "^0.11.26",
    "ollama": "^0.5.16",
    "zod": "^3.25.76",
    "zod-to-json-schema": "^3.24.6"
  }
}
